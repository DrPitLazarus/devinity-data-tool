{
    "version": 2,
    "name": "devinity-data-tool",
    "public": false,
    "env": {
        "db": "devinity",
        "host": "@host",
        "user": "@user",
        "pass": "@pass"
    },
    "builds": [
        { "src": "api/index.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "api/index.js" }
    ]
}