{
    "version": 2,
    "name": "devinity-data-tool",
    "public": false,
    "builds": [
        { "src": "api/index.js", "use": "@now/node" },
        { "src": "ui/package.json", "use": "@now/static-build" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "api/index.js" },
        { "src": "/", "dest": "ui/index.html" },
        { "src": "/(.*)", "dest": "ui/$1.html" }
    ]
}